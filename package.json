{
  "name": "themix",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.cjs",
  "type": "module",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs",
      "default": "./dist/main.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "test": "vitest -c vitest.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check {test,src}/**/*.ts docs/**/*.md {*.md,*.ts,eslint.config.js}",
    "format:fix": "prettier --write {test,src}/**/*.ts docs/**/*.md {*.md,*.ts,eslint.config.js}",
    "prebench": "npm run build",
    "test:watch": "vitest -c vitest.config.ts --watch",
    "test:coverage": "vitest -c vitest.config.ts --coverage",
    "clean": "rimraf dist coverage build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "preversion": "npm run lint && npm run test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supitsdu/climonad.js.git"
  },
  "keywords": [
    "cli",
    "command-line",
    "arguments",
    "parser",
    "high-performance",
    "nodejs",
    "typescript",
    "commander",
    "options",
    "flags",
    "terminal",
    "command",
    "argv"
  ],
  "author": "Eduardo M. Santos <contact@supitsdu.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supitsdu/climonad.js/issues"
  },
  "homepage": "https://github.com/supitsdu/climonad.js#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^3.1.2",
    "colorus-js": "^2.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-bundlens": "^0.1.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
